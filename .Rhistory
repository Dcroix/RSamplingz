# Created by Paolo G. Hilado June 23, 2018, 2018
Cochran <- function(z,p,e){
q <- 1 - p
x <- z*z
y <- x*p*q
res <- y /(e*e)
res <- ceiling(res)
print(res)
}
Cochran(1.96, 0.75, 0.05)
library(RSamplingz)
?Cochran
CochranMod <- function(no, N){
x <- no -1
y <- x / N
z <- 1 + y
n <- no / z
n <- ceiling(n)
print(n)
}
