#Created by Paolo G. Hilado April 24, 2018
systematic <- function(x,y){
k <- x/y
a <- runif(1, 1, k)
b <- round(a, digits = 0)
c <- seq(b, b+k*(y-1), k)
list <- round(c, digits = 0)
list <- list(Systematic Random Numbers = list)
}
